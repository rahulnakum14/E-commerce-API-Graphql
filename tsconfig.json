{
    "compilerOptions": {
      "lib": ["es6"],
      "module": "commonjs",
      "target": "es6",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "noImplicitAny": false,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "baseUrl": "./",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  